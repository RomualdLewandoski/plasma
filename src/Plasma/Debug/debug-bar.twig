<script>
    $(document).ready(() => {
        $('#sfToolbarHideButton').click((event) => {
            event.preventDefault()
            $('#sfToolbarMainContent').hide();
            $('#sfMiniToolbar').show();
        })
        $('#sfToolbarMiniToggler').click((event) => {
            event.preventDefault()
            $('#sfToolbarMainContent').show();
            $('#sfMiniToolbar').hide();
        })
    })
</script>
