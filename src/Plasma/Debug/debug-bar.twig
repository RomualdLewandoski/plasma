    .sf-minitoolbar {
        background-color: #222;
        border-top-left-radius: 4px;
        bottom: 0;
        box-sizing: border-box;
        display: none;
        height: 36px;
        padding: 6px;
        position: fixed;
        right: 0;
        z-index: 99999;
    }

    .sf-minitoolbar button {
        background-color: transparent;
        padding: 0;
        border: none;
    }

