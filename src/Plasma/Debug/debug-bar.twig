<div id="sfToolbarMainContent" class="sf-toolbarreset clear-fix" data-no-turbolink="" style="display: block;">

    <div class="sf-toolbar-block sf-toolbar-block-request sf-toolbar-status-normal ">

        <div class="sf-toolbar-icon">

            <span class="sf-toolbar-label"> @</span>
            <span class="sf-toolbar-value sf-toolbar-info-piece-additional" id="debug-routeName">{{ routeName }}</span>
        </div>

        <div class="sf-toolbar-info">
            <div class="sf-toolbar-info-group">
                <div class="sf-toolbar-info-piece">
                    <b>HTTP status</b>
                    <span>200 OK</span>
                </div>

                <div class="sf-toolbar-info-piece">
                    <b>Controller</b>
                    <span id="debug-controller">{{ controller }}</span>
                </div>

                <div class="sf-toolbar-info-piece">
                    <b>Route name</b>
                    <span id="debug-routeName2">{{ routeName }}</span>
                </div>
            </div>


        </div>
    </div>

    <div class="sf-toolbar-block sf-toolbar-block-time {{ loadClass }} ">
        <div class="sf-toolbar-icon">
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                <path fill="#AAA"
                      d="M15.1 4.3a13 13 0 0 0-6.2 0c-.3 0-.7-.2-.7-.5v-.4c0-1.2 1-2.3 2.3-2.3h3c1.2 0 2.3 1 2.3 2.3v.3c0 .4-.4.6-.7.6zm5.8 9.7a9 9 0 0 1-17.8 0 9 9 0 0 1 17.8 0zm-4.2 1c0-.6-.4-1-1-1H13V8.4c0-.6-.4-1-1-1s-1 .4-1 1v6.2c0 .6.4 1.3 1 1.3h3.7c.5.1 1-.3 1-.9z"></path>
            </svg>

            <span class="sf-toolbar-value" id="debug-loadTime">{{ totalLoadTime }}</span>
            <span class="sf-toolbar-label">ms</span>
        </div>

        <div class="sf-toolbar-info">
            <div class="sf-toolbar-info-piece">
                <b>Total time</b>
                <span id="debug-loadTime2">{{ totalLoadTime }} ms</span>
            </div>
            <div class="sf-toolbar-info-piece">
                <b>Initialization time</b>
                <span>{{ initTime }} ms</span>
            </div>
        </div>
    </div>


    <div class="sf-toolbar-block sf-toolbar-block-ajax sf-toolbar-status-normal">
        <div class="sf-toolbar-icon">
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                <path fill="#AAA"
                      d="M9.8 18L6 22.4c-.3.3-.8.4-1.1 0L1 18c-.4-.5-.1-1 .5-1H3V6.4C3 3.8 5.5 2 8.2 2h3.9c1.1 0 2 .9 2 2s-.9 2-2 2H8.2C7.7 6 7 6 7 6.4V17h2.2c.6 0 1 .5.6 1zM23 6l-3.8-4.5a.8.8 0 0 0-1.1 0L14.2 6c-.4.5-.1 1 .5 1H17v10.6c0 .4-.7.4-1.2.4h-3.9c-1.1 0-2 .9-2 2s.9 2 2 2h3.9c2.6 0 5.2-1.8 5.2-4.4V7h1.5c.6 0 .9-.5.5-1z"></path>
            </svg>

            <span class="sf-toolbar-value sf-toolbar-ajax-request-counter" id="ajaxCount1">{{ ajax|length }}</span>
        </div>
        <div class="sf-toolbar-info">
            <div class="sf-toolbar-info-piece">
            <span class="sf-toolbar-header">
                <b class="sf-toolbar-ajax-info" id="ajaxCount2">{{ ajax|length }} AJAX requests</b>
            </span>
            </div>
            <div class="sf-toolbar-info-piece">
                <table class="sf-toolbar-ajax-requests">
                    <thead>
                    <tr>
                        <th>#</th>
                        <th>Method</th>
                        <th>Status</th>
                        <th>URL</th>
                        <th>Time</th>
                    </tr>
                    </thead>
                    <tbody class="sf-toolbar-ajax-request-list" id="ajaxTable">
                    {% for temp in ajax %}
                        <tr>
                            <td>{{ temp.id }}</td>
                            <td>{{ temp.method }}</td>
                            <td>{{ temp.code }}</td>
                            <td>{{ temp.url }}</td>
                            <td>{{ temp.totalTime }} ms</td>
                        </tr>
                    {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>
    </div>

    <div class="sf-toolbar-block sf-toolbar-block-security sf-toolbar-status-normal ">
        <div class="sf-toolbar-icon">
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                <path fill="#AAA"
                      d="M21 20.4V22H3v-1.6c0-3.7 2.4-6.9 5.8-8-1.7-1.1-2.9-3-2.9-5.2 0-3.4 2.7-6.1 6.1-6.1s6.1 2.7 6.1 6.1c0 2.2-1.2 4.1-2.9 5.2 3.4 1.1 5.8 4.3 5.8 8z"></path>
            </svg>

            {% if isSession %}
                <span class="sf-toolbar-value" id="debug-session">{{ sessionName }}</span>
            {% else %}
                <span class="sf-toolbar-value" id="debug-session">n/a</span>
            {% endif %}
        </div>
        <div class="sf-toolbar-info">
            <div class="sf-toolbar-info-group">
                <div class="sf-toolbar-info-piece">
                    <b>Authenticated</b>
                    {% if isSession %}
                        <span class="sf-toolbar-status sf-toolbar-status-green" id="debug-sessionlog">Yes</span>
                    {% else %}
                        <span class="sf-toolbar-status sf-toolbar-status-yellow" id="debug-sessionlog">No</span>
                    {% endif %}
                </div>
            </div>
        </div>
    </div>

